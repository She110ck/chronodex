{
  "manifest_version": 3,
  "name": "Simple Chronodex Tab",
  "version": "0.1.2",
  "description": "Daily planning tool",
  "author": "Harold Finch",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
